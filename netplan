# TWO INT. NETPLAN TEMPLATE BY JULIO

network:
  ethernets:
  
      enp0s3:
        dhcp4: true
        
      enp0s8:
        dhcp4: false
        addresses: [192.168.1.101/24]
        routes:
          - to: default
            via: 192.168.1.1
        nameservers:
          addresses:
            - 127.0.0.1
            - 1.1.1.1
                  
version: 2  
